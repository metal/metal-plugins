    "prepublish": "npm run compile"
  },
  "keywords": [
    "metal"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "lint-staged": "^5.0.0",
    "webpack": "^3.11.0"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add"
    ]
  }
}
