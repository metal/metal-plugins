{
  "name": "metal-aop",
  "version": "3.0.0",
  "description": "Metal.js utility for AOP.",
  "license": "BSD",
  "repository": "https://github.com/metal/metal-plugins/tree/master/packages/metal-aop",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "main": "lib/AOP.js",
  "jsnext:main": "src/AOP.js",
  "files": [
    "lib",
    "src",
    "test"
  ],
  "scripts": {
    "checkFormat": "npm run prettier -- --list-different",
    "compile": "babel --presets env -d lib/ src/",
    "format": "npm run prettier -- --write",
    "lint": "eslint '{src,test}/**/*.js'",
    "precommit": "lint-staged",
    "prepublish": "npm run compile",
    "prettier": "prettier-eslint '{src,test}/**/*.js'",
    "test": "karma start",
    "test:saucelabs": "karma start karma-saucelabs.conf.js"
  },
  "keywords": [
    "metal"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-config-liferay": "^2.0.16",
    "karma": "^1.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sauce-launcher": "~1.1.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^2.0.6",
    "lint-staged": "^5.0.0",
    "mocha": "^4.0.1",
    "prettier-eslint-cli": "^4.4.0",
    "sinon": "^4.1.2",
    "webpack": "^3.8.1"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add"
    ]
  }
}
