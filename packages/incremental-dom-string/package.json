    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "chai": "^3.5.0",
    "eslint": "^4.11.0",
    "eslint-config-liferay": "^2.0.16",
    "gulp": "^3.9.1",
    "karma": "^1.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sauce-launcher": "~1.1.0",
    "karma-webpack": "^2.0.5",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "prettier-eslint-cli": "^4.4.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-stream": "^1.19.0",
    "sinon": "^1.17.7",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^3.5.5"
  }
