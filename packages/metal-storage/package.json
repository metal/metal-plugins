{
  "name": "metal-storage",
  "version": "1.1.0",
  "description": "Provides a convenient API for data persistence using a selected data storage mechanism",
  "license": "BSD",
  "repository": "metal/metal-plugins/tree/master/packages/metal-storage",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "jsnext:main": "src/all/storage.js",
  "main": "lib/all/storage.js",
  "files": [
    "lib",
    "src",
    "test"
  ],
  "scripts": {
    "build": "webpack",
    "checkFormat": "npm run prettier -- --list-different",
    "compile": "babel --presets env -d lib/ src/",
    "format": "npm run prettier -- --write",
    "lint": "eslint '{src,test}/**/*.js'",
    "prepublish": "npm run compile",
    "prettier": "prettier-eslint '{src,test}/**/*.js'",
    "test": "karma start",
    "test:saucelabs": "karma start karma-saucelabs.conf.js"
  },
  "keywords": [
    "metal"
  ],
  "dependencies": {
    "metal": "^2.16.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-node-env-inline": "^0.4.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.11.0",
    "eslint-config-liferay": "^2.0.16",
    "karma": "^1.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-webpack": "^2.0.5",
    "mocha": "^4.0.1",
    "prettier-eslint-cli": "^4.4.0",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^3.5.5"
  }
}
